{
  "buildCommand": "cd backend && npm install && npm run build && cd .. && next build",
  "devCommand": "npm run dev:frontend",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["gru1"],
  "env": {
    "NEXT_PUBLIC_XRPL_NETWORK": "testnet"
  },
  "functions": {
    "api/backend.js": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "backend/dist/**"
    }
  }
}
